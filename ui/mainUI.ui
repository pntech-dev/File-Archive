   <string notr="true">QMainWindow {
    background-color: #FFFFFF;    /* Белый фон окна */
}

QLabel {
    color: #0F172A;   /* Акцентный цвет текста */
}

QFrame {
    background-color: #FFFFFF;
    border: none;
}

/* === Горизонтальные линии === */
QFrame[frameShape=&quot;4&quot;] { /* QFrame::HLine */
    background-color: #E2E8F0;
    min-height: 1px;
    max-height: 1px;
    border: none;
}

/* === Вертикальные линии === */
QFrame[frameShape=&quot;5&quot;] { /* QFrame::VLine */
    background-color: #E2E8F0;
    min-width: 1px;
    max-width: 1px;
    border: none;
}



/* === Базовый стиль для всех QLineEdit === */
QLineEdit {
    background-color: #FFFFFF;       /* Белый фон */
    border: 1px solid #E2E8F0;       /* Светлая рамка */
    border-radius: 6px;              /* Скругления */
    color: #64748B;                  /* Цвет текста */
    padding: 6px 8px;                /* Внутренние отступы для аккуратности */
}

/* === При фокусе (активное поле ввода) === */
QLineEdit:focus {
    background-color: #FFFFFF;       /* Белый фон */
    border: 1px solid #2563EB;       /* Синяя рамка */
    border-radius: 6px;
    color: #0F172A;                  /* Тёмный текст */
    outline: none;                   /* Убираем системный контур */
}

/* === Отключенное состояние === */
QLineEdit:disabled {
    background-color: #FFFFFF;       /* Белый фон (не серый) */
    border: 1px solid #F8FAFC;       /* Очень светлая рамка */
    border-radius: 6px;
    color: #E2E8F0;                  /* Бледный текст */
}


/* === Второстепенные кнопки === */
QPushButton {
    background-color: transparent;   /* Без заливки */
    color: #334155;                  /* Основной цвет текста */
    font-weight: normal;             /* Обычная жирность */
    border: 1px solid #64748B;       /* Серо-синяя рамка */
    border-radius: 6px;              /* Скругления */
    padding: 6px 12px;               /* Внутренние отступы */
}

/* === Наведение (hover) === */
QPushButton:hover {
    border: 1px solid #8B98AA;       /* Более светлая рамка */
}

/* === Нажатие (pressed / checked) === */
QPushButton:pressed,
QPushButton:checked {
    border: 1px solid #515E71;       /* Более тёмная рамка */
}

/* === Отключена (disabled) === */
QPushButton:disabled {
    color: #E2E8F0;                  /* Бледный текст */
    border: 1px solid #F8FAFC;       /* Светлая рамка */
}



/* === Общий стиль выпадающих списков (QComboBox) === */
QComboBox {
    background-color: #FFFFFF;           /* Белый фон */
    border: 1px solid #E2E8F0;           /* Светлая рамка */
    border-radius: 6px;                  /* Скругления */
    padding: 4px 32px 4px 8px;           /* Отступы: справа место под иконку */
    color: #64748B;                      /* Цвет текста */
}

/* === Иконка стрелки (ресурс combobox_arrow) === */
QComboBox::down-arrow {
    image: url(:/icons/combobox_arrow);        /* Подключённая иконка */
    width: 14px;
    height: 14px;
}

/* === Позиция иконки справа === */
QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 28px;
    border: none;                        /* Без отдельной рамки */
    margin-right: 4px;
}

/* === Фокус или раскрытое состояние === */
QComboBox:focus,
QComboBox:on {
    border: 1px solid #2563EB;           /* Синяя рамка */
    color: #0F172A;                      /* Тёмный текст */
}

/* === Меняем цвет стрелки при фокусе === */
QComboBox:focus::down-arrow,
QComboBox:on::down-arrow {
    image: url(:/combobox_arrow);        /* Та же иконка, Qt не поддерживает перекраску напрямую */
}

/* Если у тебя есть возможность использовать SVG — можно будет менять цвет через fill в ресурсе */

/* === Отключённое состояние === */
QComboBox:disabled {
    background-color: #FFFFFF;
    border: 1px solid #F8FAFC;           /* Очень светлая рамка */
    color: #E2E8F0;                      /* Светлый текст */
}

QComboBox:disabled::down-arrow {
    image: url(:/combobox_arrow);        /* Та же иконка */
    /* Qt не позволяет легко изменить цвет, поэтому сделай отдельный серый SVG для disabled */
}

/* === Стиль выпадающего списка === */
QComboBox QAbstractItemView {
    background-color: #FFFFFF;           /* Белый фон меню */
    border: 1px solid #E2E8F0;           /* Та же рамка */
    border-radius: 6px;
    selection-background-color: #2563EB; /* Цвет выделения */
    selection-color: #FFFFFF;            /* Цвет текста при выделении */
}



/* === Общие стили для QRadioButton === */
QRadioButton {
    color: #0F172A;            /* Акцентный текст */
    font-weight: normal;
    spacing: 8px;              /* Расстояние между индикатором и текстом */
}

/* === Индикатор === */
QRadioButton::indicator {
    width: 14px;
    height: 14px;
}

/* === Активное === */
QRadioButton::indicator:checked {
    image: url(:/icons/radio_buttons/radio_button_checked.svg);
}

/* === Активное + наведение === */
QRadioButton::indicator:checked:hover {
    image: url(:/icons/radio_buttons/radio_button_checked_hover.svg);
}

/* === Активное + нажатие === */
QRadioButton::indicator:checked:pressed {
    image: url(:/icons/radio_buttons/radio_button_checked_pressed.svg);
}

/* === Активное Отключённое === */
QRadioButton::indicator:checked:disabled {
    image: url(:/icons/radio_buttons/radio_button_checked_disabled.svg);
}

/* === Неактивное === */
QRadioButton::indicator:unchecked {
    image: url(:/icons/radio_buttons/radio_button_unchecked.svg);
}

/* === Неактивное + наведение === */
QRadioButton::indicator:unchecked:hover {
    image: url(:/icons/radio_buttons/radio_button_unchecked_hover.svg);
}

/* === Неактивное + нажатие === */
QRadioButton::indicator:unchecked:pressed {
    image: url(:/icons/radio_buttons/radio_button_unchecked_pressed.svg);
}

/* === Неактивное Отключённое=== */
QRadioButton::indicator:unchecked:disabled {
    image: url(:/icons/radio_buttons/radio_button_unchecked_disabled.svg);
}

/* === Текст отключённой радиокнопки === */
QRadioButton:disabled {
    color: #64748B;
}



/* === Общие стили для QCheckBox === */
QCheckBox{
    color: #0F172A;                /* Акцентный текст */
    font-weight: normal;
    spacing: 8px;                  /* Расстояние между индикатором и текстом */
}

/* Индикатор */

QCheckBox::indicator {
    width: 14px;
    height: 14px;
    border-radius: 3px;            /* Небольшое скругление углов */
    border: 1px solid #64748B;     /* Второстепенная рамка */
    background-color: #FFFFFF;     /* Белый фон */
}

/* Наведение */
QCheckBox::indicator:hover {
    border: 1px solid #2563EB;     /* Акцентная рамка */
}

/* Активен (галочка) */
QCheckBox::indicator:checked {
    border: 1px solid #2563EB;
    background-color: #2563EB;
    image: url(:/icons/checkbox_check);  /* Собственная белая галочка */
}

/* Фокус */
QCheckBox::indicator:focus {
    border: 1px solid #2563EB;
}

/* Отключён */
QCheckBox::indicator:disabled {
    border: 1px solid #F8FAFC;
    background-color: #FFFFFF;
    image: none;
}

/* Текст отключённого чекбокса */
QCheckBox:disabled {
    color: #64748B;
}



/* === Общий стиль прогресс-бара === */
QProgressBar {
    border: 1px solid #E2E8F0;     /* Светлая рамка */
    border-radius: 6px;            /* Скругления */
    background-color: #FFFFFF;     /* Белый фон (без заполнения) */
    text-align: center;            /* На всякий случай, но текста нет */
    color: transparent;            /* Полностью скрываем текст */
}

/* === Полоса заполнения === */
QProgressBar::chunk {
    border-radius: 6px;            /* Та же форма, чтобы совпадала с контуром */
    background-color: #16A34A;     /* Зелёный акцент */
    margin: 0px;                   /* Без промежутков между сегментами */
}



/* === Таблица === */
QTableWidget {
    background-color: #FFFFFF;       /* Белый фон */
    border: 1px solid #E2E8F0;       /* Светло-серая рамка */
    border-radius: 6px;              /* Скругления */
    gridline-color: #E2E8F0;         /* Цвет линий между ячейками */
    outline: none;                   /* Без фокуса-рамки */
    alternate-background-color: #F8FAFC;  /* Цвет зебры */
}

/* === Заголовки === */
QHeaderView::section {
    background-color: #F8FAFC;
    color: #334155;
    border: none;
    border-right: 1px solid #E2E8F0;
	border-bottom: 1px solid #E2E8F0;
    padding: 6px 8px;
    font-weight: 600;
}

/* Последний заголовок без правой линии */
QHeaderView::section:last {
    border-right: none;
}

/* === Ячейки === */
QTableWidget::item {
    padding: 4px 8px;
}

/* === Hover (наведение) === */
QTableWidget::item:hover {
    background-color: #EEF4FF;       /* Мягкий синий оттенок */
}

/* === Стиль выделенной строки === */
QTableWidget::item:selected {
    background-color: #C8D9FF;       /* Акцент выделения */
}

/* === Скроллбар (вертикальный) === */
QScrollBar:vertical {
    border: none;
    background: transparent;
    width: 10px;
    margin: 2px;
    border-radius: 2px;
}

QScrollBar::handle:vertical {
    background: #CBD5E1;
    border-radius: 5px;
    min-height: 20px;
}

QScrollBar::handle:vertical:hover {
    background: #94A3B8;
}

QScrollBar::add-line:vertical,
QScrollBar::sub-line:vertical {
    height: 0;
}

/* === Скроллбар (горизонтальный) === */
QScrollBar:horizontal {
    border: none;
    background: transparent;
    height: 10px;
    margin: 2px;
    border-radius: 2px;
}

QScrollBar::handle:horizontal {
    background: #CBD5E1;
    border-radius: 5px;
    min-width: 20px;
}

QScrollBar::handle:horizontal:hover {
    background: #94A3B8;
}

QScrollBar::add-line:horizontal,
QScrollBar::sub-line:horizontal {
    width: 0;
}

/* === Угловая кнопка в левом верхнем углу === */
QTableCornerButton::section {
    background-color: #F8FAFC;
    border: none;
    border-right: 1px solid #E2E8F0;
    border-bottom: 1px solid #E2E8F0;
}
